{
  "name": "vcpkg-ci-paraview",
  "version-string": "1",
  "port-version": 3,
  "description": "Port to force features of certain ports within CI",
  "homepage": "https://github.com/microsoft/vcpkg",
  "dependencies": [
    {
      "name": "paraview",
      "default-features": false,
      "features": [
        "tools"
      ],
      "platform": "x86 & windows"
    },
    {
      "name": "paraview",
      "default-features": false,
      "features": [
        "tools",
        "vtkm"
      ],
      "platform": "!(x86 & windows)"
    }
  ]
}
