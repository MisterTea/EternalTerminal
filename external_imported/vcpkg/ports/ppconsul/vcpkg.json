  "homepage": "https://github.com/oliora/ppconsul",
  "dependencies": [
    "boost-core",
    "boost-fusion",
    "boost-optional",
    "boost-variant",
    {
      "name": "curl",
      "features": [
        "openssl"
      ]
    },
    "json11",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ]
}
