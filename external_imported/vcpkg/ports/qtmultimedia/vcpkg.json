{
  "name": "qtmultimedia",
  "version-semver": "6.2.2",
  "description": "Qt Multimedia",
  "homepage": "https://www.qt.io/",
  "dependencies": [
    {
      "name": "alsa",
      "platform": "linux"
    },
    {
      "name": "qtbase",
      "default-features": false,
      "features": [
        "gui",
        "network"
      ]
    },
    {
      "name": "qtshadertools",
      "host": true,
      "default-features": false
    }
  ],
  "default-features": [
    "widgets"
  ],
  "features": {
    "gstreamer": {
      "description": "Build with gstreamer",
      "dependencies": [
        "angle",
        "gstreamer"
      ]
    },
    "qml": {
      "description": "Build QML imports",
      "dependencies": [
        {
          "name": "qtdeclarative",
          "default-features": false
        }
      ]
    },
    "widgets": {
      "description": "Build Multimedia Widgets",
      "dependencies": [
        {
          "name": "qtbase",
          "default-features": false,
          "features": [
            "widgets"
          ]
        }
      ]
    }
  }
}
