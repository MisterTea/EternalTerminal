{
  "name": "numcpp",
  "version": "2.6.0",
  "description": "C++ implementation of the Python Numpy library",
  "homepage": "https://dpilger26.github.io/NumCpp",
  "license": "MIT",
  "dependencies": [
    "boost-algorithm",
    "boost-endian",
    "boost-integer",
    "boost-math",
    "boost-predef",
    "boost-random",
    "boost-type-traits",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "features": {
    "python": {
      "description": "Interacting with Python with pybind11 interface",
      "dependencies": [
        "boost-python",
        "pybind11"
      ]
    }
  }
}
