  "version-semver": "6.2.2",
  "port-version": 1,
  "description": "Qt component for application lifecycle management",
  "homepage": "https://www.qt.io/",
  "supports": "!(windows & arm)",
  "dependencies": [
    "libarchive",
    "libyaml",
    {
      "name": "qtapplicationmanager",
