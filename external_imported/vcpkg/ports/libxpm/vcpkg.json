{
  "name": "libxpm",
  "version": "3.5.14",
  "description": "XPM format pixmap library",
  "homepage": "https://github.com/freedesktop/libXpm",
  "license": "MIT",
  "supports": "windows | linux",
  "dependencies": [
    "libx11",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    "xorg-macros",
    "xproto"
  ],
  "features": {
    "gettext": {
      "description": "use xgettext to build source",
      "dependencies": [
        {
          "name": "gettext",
          "host": true,
          "features": [
            "tools"
          ]
        }
      ]
    }
  }
}
