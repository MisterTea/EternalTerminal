{
  "name": "freerdp",
  "version": "2.4.0",
  "port-version": 3,
  "description": "A free implementation of the Remote Desktop Protocol (RDP)",
  "homepage": "https://github.com/FreeRDP/FreeRDP",
  "supports": "!(arm | uwp)",
  "dependencies": [
    {
      "name": "glib",
      "platform": "!windows"
    },
    "openssl",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "features": {
    "urbdrc": {
      "description": "USB redirection channel support",
      "dependencies": [
        "libusb"
      ]
    }
  }
}
