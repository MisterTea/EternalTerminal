import("../../build/crashpad_buildconfig.gni")

config("zlib_config") {
  defines = [ "CRASHPAD_ZLIB_SOURCE_EXTERNAL" ]
  include_dirs = [ "@_INCLUDE_DIR@" ]
}

source_set("zlib") {
  public_configs = [ ":zlib_config" ]
  if(is_debug) {
    libs = [ "@_LIBRARY_DEB@" ]  
  } else {
    libs = [ "@_LIBRARY_REL@" ]
  }
}