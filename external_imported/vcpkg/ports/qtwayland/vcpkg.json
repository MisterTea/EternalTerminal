{
  "name": "qtwayland",
  "version-semver": "6.2.2",
  "description": "A toolbox for making Qt based Wayland compositors",
  "homepage": "https://www.qt.io/",
  "supports": "!windows",
  "dependencies": [
    {
      "name": "qtbase",
      "default-features": false,
      "features": [
        "gui"
      ]
    }
  ],
  "features": {
    "qml": {
      "description": "Build QML imports",
      "dependencies": [
        {
          "name": "qtdeclarative",
          "default-features": false
        }
      ]
    }
  }
}
